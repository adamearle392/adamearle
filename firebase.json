{
    "hosting": {
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [{
                "source": "/quacker",
                "destination": "/quacker.html"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}